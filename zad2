#include <iostream>
#include <math.h>
#include <ctime>

using namespace std;

int main (){

    int n=5;
    int A[n];
    int B[n];
    int liczba=1;

    cout<<"A: ";

    for (int i=0; i<n; i++){
        A[i]=liczba;
        cout<<A[i]<<" ";
        liczba += 2;
    }

    cout<<endl<<"B: ";

    for (int i=0; i<n; i++){
        B[i]=A[n-i-1];
        cout<< B[i]<<" ";
    }
    cout<<endl;

return 0;
}
